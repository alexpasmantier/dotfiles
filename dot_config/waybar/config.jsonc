  "modules-left": [
    "hyprland/workspaces",
    "mpris"
  ],
  "modules-center": [
    "clock",
    "hyprland/window"
  ],
  "modules-right": [
    "group/tray-expander",
    "memory",
    "cpu",
    "bluetooth",
    "network",
    "pulseaudio",
    "battery"
  ],
